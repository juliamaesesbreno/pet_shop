<center><h1>Log In</h1></center>
<table align="center">
<%= form_tag sessions_path do %>
  <center><div class="field">
    <%= label_tag 'Email:' %><br />
    <%= text_field_tag :email, params[:email] %>
  </div></center>
  <center><div class="field">
    <%= label_tag 'Password:' %><br />
    <%= password_field_tag :password %>
  </div></center>
  <center><button><%= link_to 'Log in', animals_path%></button></center>
<% end %>
<br><center><%= link_to 'New Account? Sign Up', signup_path %></center>